{
  "name": "@warungpintar/ninshu",
  "version": "0.0.1-beta",
  "repository": "git@github.com:warungpintar/ninshu.git",
  "author": "adeyahya <adeyahyaprasetyo@gmail.com>",
  "homepage": "https://ninshu.warpin.vercel.app/",
  "license": "MIT",
  "main": "lib/index.js",
  "unpkg": "dist/ninshu.development.js",
  "types": "types/index.d.ts",
  "module": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:types",
    "build:es": "rimraf ./es && esbuild src/*.ts src/**/*.ts --format=esm --sourcemap=external --outdir=es",
    "build:commonjs": "rimraf ./lib && esbuild src/*.ts src/**/*.ts --format=cjs --sourcemap=external --outdir=lib",
    "build:umd:minified": "esbuild src/index.ts --format=iife --sourcemap=external --bundle --minify --outfile=dist/ninshu.production.js",
    "build:umd": "rimraf ./dist && esbuild src/index.ts --format=iife --sourcemap=external --bundle --outfile=dist/ninshu.development.js && yarn build:umd:minified",
    "build:types": "rimraf ./types && tsc --project ./tsconfig.types.json && replace 'import type' 'import' ./types -r --silent && replace 'export type' 'export' ./types -r --silent",
    "benchmark": "yarn build:commonjs && perf/scripts/run.js",
    "docs": "docs-ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@rollup/plugin-replace": "^2.3.4",
    "babel-jest": "^26.6.3",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "cross-env": "^7.0.3",
    "docs-ts": "^0.6.3",
    "esbuild": "^0.8.48",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "replace": "^1.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "expressive-ts": "^0.0.2",
    "fp-ts": "^2.9.5",
    "io-ts": "^2.2.14"
  }
}
