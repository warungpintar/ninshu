{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": [
      "ESNext",
      "dom"
    ],
    "target": "es5",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@string/*": [
        "src/string/*"
      ],
      "@object/*": [
        "src/object/*"
      ]
    },
    "types": [
      "jest"
    ]
  },
  "include": [
    "./src",
    "./__tests__"
  ]
}