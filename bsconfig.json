{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "ninshu",
  "version": "0.0.1",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "warnings": {
    "error": "+101"
  },
  "gentypeconfig": {
    "language": "typescript",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "refmt": 3
}
